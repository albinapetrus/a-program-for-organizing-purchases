    "/api/Offers": {
      "post": {
        "tags": [
          "Offers"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "PaymentEdrpou",
                  "ProcurementId",
                  "ProposedDeliveryDate",
                  "ProposedPrice",
                  "SupplierBankName",
                  "SupplierContactPhone",
                  "SupplierFullName",
                  "SupplierIban"
                ],
                "type": "object",
                "properties": {
                  "ProcurementId": {
                    "type": "string",
                    "format": "uuid"
                  },
                  "ProposedPrice": {
                    "type": "string"
                  },
                  "Message": {
                    "maxLength": 1000,
                    "minLength": 0,
                    "type": "string"
                  },
                  "OfferDocument": {
                    "type": "string",
                    "format": "binary"
                  },
                  "SupplierContactPhone": {
                    "pattern": "^\\+?3?8?(0\\d{9})$",
                    "type": "string",
                    "format": "tel"
                  },
                  "ProposedDeliveryDate": {
                    "type": "string",
                    "format": "date"
                  },
                  "SupplierFullName": {
                    "maxLength": 255,
                    "minLength": 3,
                    "type": "string"
                  },
                  "PaymentEdrpou": {
                    "pattern": "^\\d{8}(\\d{2})?$",
                    "type": "string"
                  },
                  "SupplierIban": {
                    "pattern": "^UA\\d{2}[\\dA-Z]{25}$",
                    "type": "string"
                  },
                  "SupplierBankName": {
                    "maxLength": 255,
                    "minLength": 3,
                    "type": "string"
                  },
                  "PaymentIpn": {
                    "pattern": "^\\d{10}(\\d{2})?$",
                    "type": "string"
                  }
                }
              },
              "encoding": {
                "ProcurementId": {
                  "style": "form"
                },
                "ProposedPrice": {
                  "style": "form"
                },
                "Message": {
                  "style": "form"
                },
                "OfferDocument": {
                  "style": "form"
                },
                "SupplierContactPhone": {
                  "style": "form"
                },
                "ProposedDeliveryDate": {
                  "style": "form"
                },
                "SupplierFullName": {
                  "style": "form"
                },
