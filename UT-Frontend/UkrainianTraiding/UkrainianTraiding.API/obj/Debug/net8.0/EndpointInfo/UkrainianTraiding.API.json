    "/api/Auth/profile/step2": {
      "put": {
        "tags": [
          "Auth"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "FullName",
                  "Ipn",
                  "LegalStatus",
                  "Role",
                  "UserId"
                ],
                "type": "object",
                "properties": {
                  "UserId": {
                    "type": "string",
                    "format": "uuid"
                  },
                  "Role": {
                    "maxLength": 50,
                    "minLength": 0,
                    "type": "string"
                  },
                  "LegalStatus": {
                    "maxLength": 10,
                    "minLength": 0,
                    "type": "string"
                  },
                  "FullName": {
                    "maxLength": 255,
                    "minLength": 0,
                    "type": "string"
                  },
                  "Ipn": {
                    "pattern": "^[0-9]{10}$",
                    "type": "string"
                  },
                  "DateOfBirth": {
                    "type": "string",
                    "format": "date-time"
                  },
                  "PassportPhoto": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              },
              "encoding": {
                "UserId": {
                  "style": "form"
                },
                "Role": {
                  "style": "form"
                },
                "LegalStatus": {
                  "style": "form"
                },
                "FullName": {
